  <li>Heat: {{ heat }}</li>
  <li>AC: {{ ac }}</li>
  <li>WC: {{ wc }}</li>
  <li>Tv: {{ tv }}</li>
  <li>Table: {{ table }}</li>
  <li>Chair: {{ chair }}</li>
