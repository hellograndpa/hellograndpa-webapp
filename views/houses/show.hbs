<h3>User: {{user}}</h3>

<h3>House specs:</h3>
<ul>
  <li>Number of rooms: {{ rooms }}</li>
  <li>Meters: {{ m2 }}</li>
  <li>Address: {{ address.street }}</li>
  <li>{{ address.city }}</li>
  <li>{{ address.state }}</li>
  <li>{{ address.zip }}</li>

